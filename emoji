local EmojiPreloader = {}

EmojiPreloader.TREE = "ğŸŒ²"
EmojiPreloader.KEY = "ğŸ”‘"
EmojiPreloader.CHECK = "âœ…"
EmojiPreloader.CROSS = "âŒ"
EmojiPreloader.ROCKET = "ğŸš€"
EmojiPreloader.STOP = "â¹ï¸"
EmojiPreloader.RUSSIA = "ğŸ‡·ğŸ‡º"
EmojiPreloader.USA = "ğŸ‡ºğŸ‡¸"

function EmojiPreloader.preloadEmojis()
    local Players = game:GetService("Players")
    local player = Players.LocalPlayer
    local playerGui = player:WaitForChild("PlayerGui")
    
    local preloadGui = Instance.new("ScreenGui")
    preloadGui.Name = "EmojiPreloader"
    preloadGui.ResetOnSpawn = false
    preloadGui.DisplayOrder = 9999
    preloadGui.Enabled = false
    preloadGui.Parent = playerGui
    
    local emojis = {
        EmojiPreloader.TREE,
        EmojiPreloader.KEY,
        EmojiPreloader.CHECK,
        EmojiPreloader.CROSS,
        EmojiPreloader.ROCKET,
        EmojiPreloader.STOP,
        EmojiPreloader.RUSSIA,
        EmojiPreloader.USA
    }
    
    for _, emoji in ipairs(emojis) do
        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(0, 1, 0, 1)
        label.Position = UDim2.new(0, -100, 0, -100)
        label.BackgroundTransparency = 1
        label.Text = emoji
        label.TextSize = 20
        label.Font = Enum.Font.GothamBold
        label.Visible = false
        label.Parent = preloadGui
    end
    
    task.delay(0.5, function()
        if preloadGui and preloadGui.Parent then
            preloadGui:Destroy()
        end
    end)
    
    return true
end
task.spawn(function()
    EmojiPreloader.preloadEmojis()
end)

return EmojiPreloader
